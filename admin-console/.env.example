# Admin Console Authentication
# These credentials are used for the admin login page
ADMIN_USER=admin
ADMIN_PASSWORD=changeme

# JWT secret for signing session tokens
# Generate a secure random string for production
JWT_SECRET=dev-secret-change-in-prod

# Python BOM Agent API Connection
# URL where the Python API is running
PYTHON_API_URL=http://localhost:8000

# Admin API key with 'admin' scope from the Python API
# Create one with: uv run sourcing apikey create --client-id <id> --name "admin-key" --scopes "admin,all"
ADMIN_API_KEY=pbom_sk_your_api_key_here

# Express Server Port (local development only)
PORT=3001
